{
  "name": "API Reference",
  "readme": "./docs/pages/misc/api-reference.md",
  "inputFiles": ["./src"],
  "out": "./docs/pages/api/",
  "publicPath": "/api/",
  "mode": "file",
  "includeDeclarations": false,
  "exclude": [
    "**/*.test-d.ts",
    "**/*.test.ts",
    "**/*.test.js",
    "**/index.ts",
    "**/main.ts",
    "docs/*",
    "dist/*"
  ],
  "disableSources": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "stripInternal": true,
  "excludeNotExported": true,
  "hideBreadcrumbs": true
}
